{
  "name": "model_document",
  "version": "0.0.1",
  "description": "model_document",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "src/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t model_document .",
    "docker:run": "docker run -p 3000:3000 -d model_document",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r dotenv/config .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "AminaLachheb <amina.lachheb.fst@gmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.1",
    "@loopback/authentication-jwt": "^0.9.1",
    "@loopback/boot": "^3.4.1",
    "@loopback/core": "^2.16.1",
    "@loopback/repository": "^3.7.0",
    "@loopback/rest": "^9.3.1",
    "@loopback/rest-explorer": "^4.1.0",
    "@loopback/service-proxy": "^3.2.1",
    "ajax": "0.0.4",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "blob": "^0.1.0",
    "chrome-launcher": "^0.14.0",
    "convertir-nombre-lettre": "^1.0.2",
    "dotenv": "^10.0.0",
    "downloadjs": "^1.4.7",
    "file-download": "^0.1.2",
    "file-saver": "^2.0.5",
    "got": "^11.8.2",
    "image-to-base64": "^2.2.0",
    "loopback-component-storage": "^3.6.0",
    "loopback-component-storage-mongo": "^1.5.1",
    "loopback-connector-mongodb": "^5.6.0",
    "loopback-filters": "^1.1.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.3",
    "pdf-creator-node": "^2.2.3",
    "pdf-lib": "^1.16.0",
    "pdf-to-base64": "^1.0.3",
    "promise-filter": "^1.1.0",
    "stream": "0.0.2",
    "stv": "^0.0.2",
    "to-words": "^2.5.0",
    "tslib": "^2.0.0",
    "ttf2woff2": "^4.0.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.1",
    "@loopback/eslint-config": "^10.2.1",
    "@loopback/testlab": "^3.4.1",
    "@types/jquery": "^3.5.6",
    "@types/multer": "^1.4.7",
    "@types/node": "^10.17.60",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.1",
    "eslint": "^7.28.0",
    "source-map-support": "^0.5.19",
    "typescript": "~4.3.2"
  }
}
